<%= alert %>

<h1>Securities Lookup</h1>

<%= form_with model: @security, url: './show', method: 'post' do |f| %>
    <p><%= f.label :symbol %></p>
    <%= f.text_field :symbol %>
    <p><%= f.label :company_name %></p>
    <%= f.text_field :company_name %><br>
    <%= f.submit value: "Lookup Security", class: 'submit-btn' %>
<% end %>
<br>
<h5><i>Only one field required, if both are populated priority will go to symbol.</i></h5>